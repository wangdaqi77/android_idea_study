<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.wongki.modularization">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY" /> <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" /> <!-- 外部存储读权限，调试工具加载本地补丁需要 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MapActivity" />
        <activity android:name=".CustomRecyclerViewActivity" /> <!-- <activity android:name=".MaterialDesignActivity"></activity> -->
        <activity android:name=".SmallRedBookActivity" />
        <activity android:name=".CustomViewActivity" />
        <activity android:name=".GlideTestActivity" />
        <activity android:name=".tinker.BugActivity" />
        <activity android:name=".MaterialDesignActivity">
        </activity>
        <activity android:name=".tinker.TestHotFixMainActivity" />
        <activity
            android:name=".keep.KeepActivity"
            android:excludeFromRecents="true"
            android:taskAffinity="com.example.wongki.modularization.keep"
            android:theme="@style/KeepTheme" />

        <service android:name=".keep.ForegroundService" />
        <service android:name=".keep.ForegroundService$InnerService" />
        <service android:name=".keep.LocalService" />
        <service
            android:name=".keep.RemoteService"
            android:process=":remote_keep" />
        <service
            android:name=".keep.KeepJobService"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <meta-data
            android:name="com.taobao.android.hotfix.IDSECRET"
            android:value="25945607" />
        <meta-data
            android:name="com.taobao.android.hotfix.APPSECRET"
            android:value="0b2a807ba79985c07c28ca6bd504354b" />
        <meta-data
            android:name="com.taobao.android.hotfix.RSASECRET"
            android:value="" />
    </application>

</manifest>